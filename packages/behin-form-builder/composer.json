{
    "name": "my/form-builder",
    "description": "A simple and elegant form builder for Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MyFormBuilder\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/view": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MyFormBuilder\\FormBuilderServiceProvider"
            ],
            "aliases": {
                "Form": "MyFormBuilder\\Facades\\Form"
            }
        }
    }
}
